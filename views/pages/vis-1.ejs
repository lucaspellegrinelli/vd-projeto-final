<canvas id="chart"></canvas>

<script>
  const data = {
    labels: [1, 2, 3],
    datasets: [
      {
        data: [1, 2, 3],
        borderColor: "#ff6384",
        backgroundColor: "#ff6384",
        label: "Linha rosa"
      },
      {
        data: [2, 4, 6],
        borderColor: "#36a2eb",
        backgroundColor: "#36a2eb",
        label: "Linha azul"
      }
    ]
  };

  const chartContext = document.getElementById("chart").getContext("2d");
  const chart = new Chart(chartContext, { type: "line", data });
</script>